class Motor {
    void start() {
        System.out.println("Motor starts");
    }
}
class Vehicle extends Motor {
    void run() {
        System.out.println("Vehicle runs using motor");
    }
}
class Bike extends Vehicle {
    void ride() {
        System.out.println("Riding the bike");
    }
}
public class Inheritance {
    public static void main(String[] args) {
        Bike bike = new Bike();
        bike.start(); 
        bike.run();   
        bike.ride(); 
    }
}
