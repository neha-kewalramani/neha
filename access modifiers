class Student {
    private String name;
    private void display() {
        System.out.println("Name: " + name);
    }
    public void setName(String n) {
        name = n;
    }
    public void show() {
        display();
    }
}
public class Main {
    public static void main(String[] args) {
        Student s = new Student();
        s.setName("Neha");
        s.show();
    }
}
