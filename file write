package test;

import java.io.File;
import java.io.FileWriter;
import java.io.IOException;

public class Filewrite{
		public static void main (String[] args) {
			FileWriter myWriter;
			 try {
				myWriter = new FileWriter("C:\\Users\\HP\\Desktop\\neha_O4\\Example.txt");
				myWriter.write("We wrote some text in the file!");
					 System.out.println("File is written");
					 myWriter.close();
				}catch (IOException e) {
				System.out.println("Error Occured");
				e.printStackTrace();
			}
		}
}
