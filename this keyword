class Student {
    String name;
    int rollNo;

    Student(String name, int rollNo) {
        this.name = name;
        this.rollNo = rollNo;
    }
    void display() {
        System.out.println("Name: " + name);
        System.out.println("Roll No: " + rollNo);
    }
}
public class Main {
    public static void main(String[] args) {
        Student s1 = new Student("Neha", 101);
        s1.display();
    }
}
